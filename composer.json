{
    "name": "gohostie/oauth-ebay-client",
    "description": "OAuth 2.0 eBay Client Library",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^5.6|^7.0",
        "guzzlehttp/guzzle": "^6.0",
        "paragonie/random_compat": "^1|^2|^9.99"
        "league/oauth2-client": "^2.0",
    },
    "require-dev": {
        "eloquent/liberator": "^2.0",
        "eloquent/phony-phpunit": "^1.0|^3.0",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "phpunit/phpunit": "^5.7|^6.0",
        "squizlabs/php_codesniffer": "^2.3|^3.0"
    },
    "keywords": [
        "oauth",
        "oauth2",
        "authorization",
        "authentication",
        "idp",
        "identity",
        "sso",
        "single sign on"
    ],
    "authors": [
        {
            "name": "ste davies",
            "email": "info@gohostie.co.uk",
            "homepage": "https://www.gohostie.co.uk",
            "role": "Developer"
        },
        {
            "name": "Woody Gilk",
            "homepage": "https://github.com/gohostie",
            "role": "Contributor"
        }

    ],
    "autoload": {
        "psr-4": {
            "Gohostie\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gohostie\\OAuth2\\Client\\Test\\": "test/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-1.x": "1.0.x-dev"
        }
    }
}
